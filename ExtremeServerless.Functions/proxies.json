{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "save": {
      "matchCondition": {
        "route": "/api/save",
        "methods": [
          "POST"
        ]
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/api/addchatmessage"
    },
    "config": {
      "matchCondition": {
        "route": "/api/config",
        "methods": [
          "GET",
          "OPTIONS"
        ]
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/api/getsignalrconfiguration"
    },
    "le": {
      "matchCondition": {
        "route": "/.well-known/acme-challenge/{*code}",
        "methods": [
          "GET",
          "PUT",
          "POST",
          "DELETE",
          "OPTIONS"
        ]
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/api/letsencrypt/{code}"
    },
    "default": {
      "matchCondition": {
        "route": "",
        "methods": [
          "GET",
          "OPTIONS"
        ]
      },
      "backendUri": "https://xtremeserverlessfiles.z6.web.core.windows.net/"
    },
    "files": {
      "matchCondition": {
        "route": "{*path}",
        "methods": [
          "GET",
          "OPTIONS"
        ]
      },
      "backendUri": "https://xtremeserverlessfiles.z6.web.core.windows.net/{path}"
    }
  }
}